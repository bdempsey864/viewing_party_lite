<div class="content">
  <h1>Viewing Party Lite</h1>
  <%= button_to "Discover Movies", "/users/#{@user.id}/discover", method: :get %>
  <br>
  <h2><%= @user.name %>'s Dashboard</h2>


  <h3>Viewing Parties</h3>
  <% @user.parties.each do |party|  %>
    <%= party.date %><br>
    <%= party.start_time %>
  <% end %>
</div>